#include <iostream>
using namespace std;
int main()
{
    int arr[5]={1,3,5,7,9};
    int key; 
    cout<<"Element to search";
    cin>>key;
    for(int i=0;i<5;i++){
      if(arr[i]==key)
      {
        cout <<"Element found at index"<<i;
        return 0;
      }
    }
    cout<<"element not found";
}
